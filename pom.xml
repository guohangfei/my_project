    <!--父类聚合项目-->
    <modules>
        <module>my_base</module>
        <module>my_log</module>
        <module>my_cms</module>
        <module>my_util</module>
        <module>my_spider</module>
    </modules>

    <!--版本控制-->
    <properties>
